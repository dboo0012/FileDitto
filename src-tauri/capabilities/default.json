{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "opener:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-create",
    "fs:allow-remove",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:allow-execute",
    "shell:default",
    "shell:default"
  ]
}